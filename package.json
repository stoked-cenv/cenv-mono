{
  "name": "stoked-cenv",
  "version": "2.0.0-a.39",
  "private": true,
  "scripts": {
    "build": "pnpm -r run build",
    "build:all": "pnpm -r run build",
    "lint": "pnpm -r run lint",
    "lint:fix": "pnpm -r run lint:fix",
    "report": "istanbul report --include coverage.json --dir f3 html",
    "list-dep-mismatches": "syncpack list-mismatches",
    "fix-dep-mismatches": "syncpack fix-mismatches",
    "e2e": "nx e2e integration",
    "clean": "rimraf --glob src/**/*.js",
    "version": "echo pnpm $(pnpm --version); echo tsc $(tsc --version); echo node $(node --version); nx --version; cenv --version",
    "version:bump": "pnpm -r run version:bump prerelease --force && pnpm -r up \"@stoked-cenv/*\" && git add package.json packages/cli/package.json packages/ui/package.json packages/lib/package.json packages/cdk/package.json pnpm-lock.yaml && git commit -m \"push bump packages\" && git push && pnpm version prerelease && git push",
    "publish:bump": "pnpm run version:bump && pnpm --filter \"@stoked-cenv/*\" run publish:bump --publish-branch $(git rev-parse --abbrev-ref HEAD)"
  },
  "husky": {
    "hooks": {
      "pre-push": "pnpm run lint"
    }
  },
  "devDependencies": {
    "@nestjs/common": "10.0.0",
    "@nestjs/core": "10.0.0",
    "@nestjs/testing": "10.0.0",
    "@nrwl/js": "16.5.5",
    "@nrwl/node": "16.5.5",
    "@nrwl/nx-cloud": "15.3.5",
    "@nrwl/workspace": "16.5.5",
    "@swc/core": "1.3.70",
    "@swc/register": "0.1.10",
    "@types/jest": "^28.1.8",
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "c8": "8.0.0",
    "concurrently": "^8.2.0",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.17.19",
    "eslint": "^8.45.0",
    "hanbi": "1.0.1",
    "jest": "^28.1.3",
    "nest-commander": "3.11.0",
    "nest-commander-testing": "^3.3.0",
    "nx": "16.5.5",
    "nx-uvu": "1.3.0",
    "rimraf": "4.4.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.0",
    "typescript": "4.9.5",
    "uvu": "^0.5.6"
  },
  "cli": {
    "packageManager": "pnpm"
  },
  "license": "UNLICENSED"
}
