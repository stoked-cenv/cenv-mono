{
  "name": "stoked-cenv",
  "version": "2.0.0-a.18",
  "private": true,
  "scripts": {
    "publish": "pnpm -r run publish",
    "build": "pnpm -r --filter ...@stoked-cenv/lib run build",
    "build:all": "pnpm -r run build",
    "lint": "pnpm -r run lint",
    "lint:fix": "pnpm -r run lint:fix",
    "report": "istanbul report --include coverage.json --dir f3 html",
    "list-dep-mismatches": "syncpack list-mismatches",
    "fix-dep-mismatches": "syncpack fix-mismatches",
    "version": "pnpm version prerelease && pnpm -r run version prerelease"
  },
  "husky": {
    "hooks": {
      "pre-push": "pnpm run lint"
    }
  },
  "dependencies": {
    "@types/jest": "^28.1.3",
    "@types/node": "18.11.18",
    "esbuild": "^0.17.15",
    "jest": "^28.1.3",
    "rimraf": "4.4.1",
    "ts-node": "^10.9.1",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "concurrently": "^8.2.0",
    "eslint": "^8.44.0",
    "tslib": "^2.6.0"
  },
  "cli": {
    "packageManager": "pnpm"
  },
  "license": "UNLICENSED"
}
