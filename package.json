{
  "name": "stoked-cenv",
  "version": "2.0.0-a.15",
  "private": true,
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "publish": "nx run-many --target=publish --all",
    "build": "nx run-many --target=build --all",
    "build:skip-cache": "nx run-many --target=build --all --skip-nx-cache",
    "lint": "nx run-many --target=lint --all",
    "lint:fix": "nx run-many --target=lint:fix --all",
    "build:since": "nx affected --target=build",
    "start": "lerna run start --stream",
    "test": "lerna run test",
    "test:unit": "lerna run test:unit",
    "lerna-publish": "lerna version patch --yes --force-publish --no-git-tag-version",
    "graph": "lerna-dependency-graph",
    "report": "istanbul report --include coverage.json --dir f3 html",
    "coverage": "yarn coverage-merge && yarn report",
    "test:integration": "lerna run test:integration",
    "list-dep-mismatches": "syncpack list-mismatches",
    "fix-dep-mismatches": "syncpack fix-mismatches",
    "clean:dependencies": "lerna exec -- rm -rf node_modules",
    "clean:lock": "lerna exec -- rm -rf yarn.lock package-lock.json; rm -rf yarn.lock package-lock.json",
    "clean:complete": "yarn run clean:cdk; yarn run clean:dependencies; yarn run clean:lock; yarn run createLocks",
    "setVersion": "version.sh",
    "tsc-version": "tsc -v"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint"
    }
  },
  "dependencies": {
    "@nrwl/nx-cloud": "16.0.5",
    "@nrwl/workspace": "15.9.4",
    "@types/jest": "^28.1.3",
    "@types/node": "18.11.18",
    "esbuild": "^0.17.15",
    "jest": "^28.1.3",
    "nx": "15.9.4",
    "rimraf": "4.4.1",
    "ts-node": "^10.9.1",
    "typescript": "4.9.5"
  },
  "cli": {
    "packageManager": "yarn"
  },
  "workspaces": {
    "packages": [
      "packages/*",
      "packages/params/*"
    ]
  },
  "license": "UNLICENSED",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.44.0"
  }
}
