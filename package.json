{
  "name": "stoked-cenv",
  "version": "2.0.0-a.112",
  "scripts": {
    "cenv": "nx run @stoked-cenv/cli:cenv",
    "publish:link": "pnpm run build",
    "publish:cli": "nx run @stoked-cenv/cli:package",
    "build": "ts-node fixup-deps.ts; nx affected:build; ts-node ./fixup-dist.ts; pnpm run link:cli",
    "build:no-cache": "ts-node fixup-deps.ts; nx affected:build --skip-nx-cache; ts-node ./fixup-dist.ts; pnpm run link:cli",
    "rebuild": "pnpm -r exec npx rimraf node_modules pnpm-lock.yaml; pnpm i; pnpm run build",
    "build:all": "pnpm -r run build",
    "lint": "pnpm -r run lint",
    "lint:fix": "pnpm -r run lint:fix",
    "report": "istanbul report --include coverage.json --dir f3 html",
    "list-dep-mismatches": "syncpack list-mismatches",
    "fix-dep-mismatches": "syncpack fix-mismatches",
    "clean": "rimraf --glob src/**/*.js",
    "test": "nx run @stoked-cenv/cli:test",
    "cli": "nx run @stoked-cenv/cli:cli",
    "update:example": "cd example && pnpm update:api",
    "update:global": "pnpm up --global",
    "link:lib": "cd packages/lib && pnpm link --global",
    "link:cdk": "cd packages/cdk && pnpm link --global",
    "link:ui": "cd packages/ui && pnpm link --global",
    "link:cli": "cd packages/cli && pnpm link --global",
    "link:all": " pnpm link:lib; pnpm link:ui; pnpm link:cdk; pnpm link:cli",
    "unlink:lib": "cd packages/lib && pnpm rm --global @stoked-cenv/lib",
    "unlink:cdk": "cd packages/cdk && pnpm rm --global @stoked-cenv/cdk",
    "unlink:ui": "cd packages/ui && pnpm rm --global @stoked-cenv/ui",
    "unlink:cli": "cd packages/cli && pnpm rm --global @stoked-cenv/cli",
    "unlink:all": "pnpm unlink:lib; pnpm unlink:ui; pnpm unlink:cdk; pnpm unlink:cli",
    "updatePeers:ui": "cd packages/ui && pnpm add @stoked-cenv/lib@\"$(cenv -v -s)\" --save-peer",
    "updatePeers:cli": "cd packages/cli && pnpm add @stoked-cenv/lib@\"$(cenv -v -s)\" --save-peer && pnpm add @stoked-cenv/ui@\"$(cenv -v -s)\" --save-peer",
    "updatePeers:cdk": "cd packages/cdk && pnpm add @stoked-cenv/lib@\"$(cenv -v -s)\" --save-peer",
    "updatePeers": "pnpm updatePeers:ui; pnpm updatePeers:cli; pnpm updatePeers:cdk",
    "version:deps": "echo pnpm $(pnpm --version); echo tsc $(tsc --version); echo node $(node --version); nx --version; cenv --version",
    "version:bump:cli": "cd packages/cli && pnpm version prerelease --force",
    "version:bump:cli-select": "cd packages/cli-select && pnpm version prerelease --force",
    "version:bump:lib": "cd packages/lib && pnpm run version:bump prerelease --force",
    "version:bump:ui": "cd packages/ui && pnpm run version:bump prerelease --force",
    "version:bump:cdk": "cd packages/cdk && pnpm run version:bump prerelease --force",
    "version:bump:example": "cd example && pnpm version:bump",
    "version:bump:all": "pnpm version:bump:cli && pnpm version:sync",
    "version:set:cli": "cd packages/cli && npm version --allow-same-version --include-workspace-root --no-commit-hooks --no-git-tag-version",
    "version:set:lib": "cd packages/lib && npm version --allow-same-version --include-workspace-root --no-commit-hooks --no-git-tag-version",
    "version:set:cli-select": "cd packages/cli-select && npm version --allow-same-version --include-workspace-root --no-commit-hooks --no-git-tag-version",
    "version:set:cdk": "cd packages/cdk && npm version --allow-same-version --include-workspace-root --no-commit-hooks --no-git-tag-version",
    "version:set:ui": "cd packages/ui && npm version --allow-same-version --include-workspace-root --no-commit-hooks --no-git-tag-version",
    "version:set:root": "npm version --allow-same-version --include-workspace-root --no-commit-hooks --no-git-tag-version",
    "version:sync": "version=$(cenv -s -v) && pnpm version:set:root $version && pnpm version:set:lib $version && pnpm version:set:ui $version && pnpm version:set:cdk $version && pnpm version:set:cli-select $version",
    "version:set:example": "cd example && npm version --allow-same-version --include-workspace-root --no-commit-hooks --no-git-tag-version",
    "version:set:api": "cd example/packages/api && npm version --allow-same-version --include-workspace-root --no-commit-hooks --no-git-tag-version",
    "version:set:spa": "cd example/packages/spa && npm version --allow-same-version --include-workspace-root --no-commit-hooks --no-git-tag-version",
    "version:set:global": "cd example/packages/globals && npm version --allow-same-version --include-workspace-root --no-commit-hooks --no-git-tag-version",
    "version:example:sync": "version=$(cenv -s -v) && pnpm version:set:example $version && pnpm version:set:api $version && pnpm version:set:global $version && pnpm version:set:spa $version",
    "publish:bump:lib": "cd dist/lib && npm publish --access public",
    "publish:bump:ui": "cd dist/ui && npm publish --access public",
    "publish:bump:cdk": "cd dist/cdk && npm publish --access public",
    "publish:bump:cli": "cd dist/cli && npm publish --access public",
    "publish:bump:cli-select": "cd dist/cli-select && npm publish --access public",
    "publish:bump:all": "pnpm run publish:bump:cli-select && pnpm run publish:bump:lib && pnpm run publish:bump:ui && pnpm run publish:bump:cdk && pnpm run publish:bump:cli"
  },
  "husky": {
    "hooks": {
      "pre-push": "pnpm run lint"
    }
  },
  "dependencies": {
    "blessed": "^0.1.81",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/common": "10.0.0",
    "@nestjs/core": "10.0.0",
    "@nestjs/testing": "10.0.0",
    "@nrwl/js": "16.5.5",
    "@nrwl/node": "16.5.5",
    "@nrwl/nx-cloud": "15.3.5",
    "@nrwl/workspace": "16.5.5",
    "@stoked-cenv/lib": "^2.0.0-a.112",
    "@swc/core": "1.3.70",
    "@swc/register": "0.1.10",
    "@types/jest": "^28.1.8",
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "c8": "8.0.0",
    "concurrently": "^8.2.0",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.17.19",
    "eslint": "^8.45.0",
    "hanbi": "1.0.1",
    "jest": "^29.1.1",
    "nest-commander": "3.11.0",
    "nest-commander-testing": "^3.3.0",
    "nx": "16.5.5",
    "nx-uvu": "1.3.0",
    "rimraf": "4.4.1",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.0",
    "typescript": "4.9.5",
    "uvu": "^0.5.6"
  },
  "cli": {
    "packageManager": "pnpm"
  },
  "contributors": [
    {
      "name": "Brian Stoker",
      "url": "https://github.com/brian-stoker",
      "githubUsername": "brian-stoker"
    }
  ],
  "peerDependencies": {
    "@stoked-cenv/lib": "^2.0.0-a.112"
  },
  "license": "UNLICENSED"
}