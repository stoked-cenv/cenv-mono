{
  "name": "@stoked-cenv/params",
  "version": "2.0.0-a.16",
  "private": true,
  "description": "Materialize environment specific configurationProfiles when triggered by an application-environment-version base configurationProfile.",
  "main": "index.js",
  "scripts": {
    "clean": "npx rimraf build materializationLambda.zip yarn.lock node_modules",
    "preinstall": "yarn run clean",
    "build": "./build.sh; yarn run clean"
  },
  "keywords": [
    "AWS",
    "AppConfig",
    "Lambda",
    "SNS",
    "Deployment",
    "ConfigurationProfile"
  ],
  "author": "Brian Stoker",
  "license": "MIT",
  "dependencies": {
    "@stoked-cenv/lib": "2.0.0-a.16",
    "js-yaml": "^4.1.0",
    "rimraf": "^4.4.1",
    "async-mutex": "^0.4.0"
  },
  "workspaces": {
    "packages": [],
    "nohoist": ["**"]
  },
  "skipDeployBuild": true,
  "verifyStack": "cenv i --verify",
  "deployStack": "cenv i -cenv",
  "destroyStack": "cenv u -cenv"
}
