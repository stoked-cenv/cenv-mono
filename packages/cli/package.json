{
  "name": "@stoked-cenv/cenv-cli",
  "version": "1.2.3-alpha.0",
  "description": "",
  "author": "Brian Stoker",
  "license": "MIT",
  "main": "dist/main.js",
  "bin": {
    "cenv": "dist/main.js"
  },
  "scripts": {
    "prebuild": "rimraf dist tsconfig.tsbuildinfo",
    "build": "npm unlink @stoked-cenv/cenv-cli -g && tsc && npm link",
    "package": "tsc && webpack --config webpack.config.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --cdoverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@stoked-cenv/cenv-ui": "*",
    "@stoked-cenv/cenv-lib": "*",
    "@nestjs/common": "^9.1.6",
    "@nestjs/core": "^9.1.6",
    "@nestjs/platform-express": "^9.0.0",
    "@types/inquirer": "^8.1.3",
    "chalk": "^4.1.0",
    "eslint": "^8.36.0",
    "nest-commander": "^3.1.0",
    "nest-winston": "^1.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.4.1",
    "rxjs": "^7.2.0",
    "ts-node": "^10.9.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.4.3",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.1.6",
    "@types/express": "^4.17.14",
    "@types/jest": "^28.1.3",
    "@types/node": "^18.11.18",
    "@types/read": "^0.0.29",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/experimental-utils": "^5.43.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@typescript-eslint/utils": "^5.54.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.3",
    "nest-commander-testing": "^3.0.1",
    "prettier": "^2.3.2",
    "process": "^0.11.10",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.2.3",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.9.5",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "jestOld": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "nx": {
    "tags": [
      "ec:tool"
    ]
  }
}
