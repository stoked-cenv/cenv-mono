{
  "name": "@stoked-cenv/web-api",
  "version": "2.0.0-a.38",
  "scripts": {
    "build": "which nest; nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cenv exec nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "link:local": "pnpm link ../../../packages/lib && pnpm link ../../../packages/cdk",
    "unlink:local": "pnpm unlink ../../../packages/lib && pnpm unlink ../../../packages/cdk",
    "lint": "eslint \"{src,apps,libs}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@stoked-cenv/cdk": "2.0.0-a.38",
    "@stoked-cenv/lib": "2.0.0-a.38",
    "express-oauth2-jwt-bearer": "^1.3.0",
    "helmet": "^6.0.1",
    "nocache": "^3.0.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.13",
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.8.4",
    "source-map-support": "^0.5.20",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4"
  },
  "deployDependencies": [
    "@stoked-cenv/cdk#cert@cenv",
    "@stoked-cenv/cdk#network"
  ],
  "destroyDependencies": [
    "@stoked-cenv/cdk#cert@cenv"
  ],
  "preBuildScripts": [
    "pnpm run unlink:local"
  ],
  "postBuildScripts": [
    "pnpm run link:local"
  ],
  "cenv": {
    "stack": {
      "package": "@stoked-cenv/cdk#api",
      "assignedSubDomain": "api.cenv",
      "volatileContextKeys": [
        "vpc-provider:account=CDK_DEFAULT_ACCOUNT:filter.tag:Name=ENV-net:region=CDK_DEFAULT_REGION:returnAsymmetricSubnets=true"
      ],
      "certArnName": "LocDashCertificateArn"
    },
    "docker": {
      "context": "../..",
      "file": "./Dockerfile"
    }
  },
  "cli": {
    "packageManager": "pnpm"
  }
}
