APPLICATION_NAME: '@stoked-cenv/web-api'
HEALTH_CHECK_PATH: /<{CENV_API_VERSION}>
ISSUER_BASE_URL: https://<{CENV_AUTH0_DOMAIN}>
APP: <{CENV_APP}>
global:
  - DOMAIN
  - CENV_APP
  - CENV_APP_DOMAIN
  - CENV_ENVIRONMENT_DOMAIN
  - CENV_API_DOMAIN
  - CENV_API_VERSION
  - CENV_AUTH0_CLIENT_ID
  - CENV_AUTH0_DOMAIN
  - CENV_AUTH0_AUDIENCE
globalEnv:
  - ENVIRONMENT_NAME
